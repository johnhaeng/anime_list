<% if flash[:error] %>
  <li><%= flash[:error] %></li>
<% end %>
<div>
<%= form_tag "/new_account" do %>
<div>
  <%= label_tag("name") %>
  <%= text_field_tag("user[name]") %>
  </div>
<div>
  <div>
    <%= label_tag("gender") %>
    <%= text_field_tag("user[gender]") %>
    </div>
  <div>
    <%= label_tag("age") %>
    <%= number_field_tag("user[age]") %>
    </div>
  <%= label_tag("email") %>
  <%= text_field_tag("user[email]") %>
  </div>
  <div>
    <%= label_tag("password") %>
  <%= password_field_tag("user[password]") %>
  </div>
  <div>
   <%= submit_tag "Create Account"%>
   </div>

<% end %>
</div>
